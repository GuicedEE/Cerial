# GuicedCerial environment placeholders â€” see rules/generative/platform/secrets-config/env-variables.md for the canonical list
ENVIRONMENT=dev
PORT=8080
SERVICE_NAME=guiced-cerial
BASE_URL=http://localhost:8080
LOGGING_BACKEND=cloud-logging
TRACING_ENABLED=true
GCP_TRACE_EXPORT=true

# OAuth / Auth (if the hosting application wires authentication)
OAUTH2_ISSUER_URL=https://auth.example.com
OAUTH2_REALM=cerial
OAUTH2_CLIENT_ID=guiced-cerial
OAUTH2_CLIENT_SECRET=***
JWKS_URI=https://auth.example.com/realms/cerial/protocol/openid-connect/certs
INTROSPECTION_URL=https://auth.example.com/realms/cerial/protocol/openid-connect/token/introspect
AUTH_ADMIN_USER=admin
AUTH_ADMIN_PASS=***

# Database placeholders (unused by this module but kept for downstream consistency)
DB_URL=jdbc:postgresql://localhost:5432/guicedee
DB_USER=guicedee
DB_PASS=***

# Testing toggles
ENABLE_DEBUG_LOGS=false
SKIP_INTEGRATION_TESTS=false
